@model PayrollSystem.Models.SalaryClass


  @if(Model != null)
  {
    foreach (var month in Model.GetMonthListByYear)
    {
        @Html.CheckBox(month.MonthId.ToString(), new { @id = month.MonthId.ToString(), @Name = "chkMonthList", @value=month.MonthId.ToString(), onclick = "ToggleGenerateBtn();" });
        @Html.Label(month.MonthName);
        <br />
    }
  }
    else
  {
      <br />
  }
